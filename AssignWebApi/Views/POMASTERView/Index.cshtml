@model IEnumerable<AssignWebApi.Models.POMASTERViewModel>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.SUPPLIER.SUPLNAME)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PODATE)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.SUPPLIER.SUPLNAME)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PODATE)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.PONO.Trim() }) |
                @Html.ActionLink("Details", "Details", new { id = item.PONO.Trim() }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.PONO.Trim() })
            </td>
        </tr>
    }

</table>
@if (!string.IsNullOrEmpty(ViewBag.ErrorMessage))
{
    <div class="alert-danger">
        @ViewBag.ErrorMessage
    </div>
}
